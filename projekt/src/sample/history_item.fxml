<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>

<fx:root prefHeight="400.0" prefWidth="880.0" style="-fx-background-color: #FFFFFF;" stylesheets="@CSS/main_theme.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="orderNumber" layoutX="14.0" layoutY="14.0" styleClass="mediumlabel" text="Order Number" />
      <Label fx:id="orderDate" layoutX="268.0" layoutY="14.0" styleClass="mediumlabel" text="Date" />
      <ScrollPane fx:id="historyScroll" hbarPolicy="NEVER" layoutY="50.0" prefHeight="350.0" prefWidth="880.0">
         <content>
            <FlowPane fx:id="orderMain" alignment="TOP_CENTER" hgap="20.0" prefHeight="350.0" prefWidth="880.0" vgap="30.0">
               <padding>
                  <Insets bottom="20.0" top="20.0" />
               </padding></FlowPane>
         </content>
      </ScrollPane>
      <ImageView fx:id="expandButton" fitHeight="36.0" fitWidth="36.0" layoutX="830.0" layoutY="10.0" onMouseClicked="#changeHistorySize" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/baseline_expand_more_black_18dp.png" />
         </image>
      </ImageView>
   </children>
</fx:root>
